# (Use -I to specify include path)
fixedpoint:
	g++ -Wall fixedpoint.cpp -o fixedpoint.out

newtonraphson:
	g++ -Wall newtonraphson.cpp -o newtonraphson.out

secant:
	g++ -Wall secant.cpp -o secant.out

bisection:
	g++ -Wall bisection.cpp -o bisection.out

falseposition:
	g++ -Wall falseposition.cpp -o falseposition.out

.PHONY: clean
clean:
	rm -fv *.o *.out